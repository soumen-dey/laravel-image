{
    "name": "soumen-dey/laravel-image",
    "description": "A package to encode and generate thumbnail for Laravel 5.6 and above.",
    "license": "MIT",
    "authors": [
        {
            "name": "Soumen Dey",
            "email": "soumendeyemail@gmail.com",
            "homepage": "http://soumendey.com"
        }
    ],
    "homepage": "https://github.com/soumen-dey/laravel-image",
    "keywords": ["Laravel", "Image"],
    "require": {
        "illuminate/support": "~5.6",
        "intervention/image": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Soumen\\Image\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Soumen\\Image\\ImageServiceProvider"
            ],
            "aliases": {
                "Image": "Soumen\\Image\\Facades\\Image"
            }
        }
    }
}
